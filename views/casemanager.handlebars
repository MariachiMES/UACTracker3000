<div class="modal" id="edit-team-lead-modal">
  <div class="modal-background" id="edit-team-lead-BG"></div>
  <div class="modal-card">
    <header class="modal-card-head">
      <p class="modal-card-title">Edit Team Lead<span class="is-hidden" name="team-lead" id="team-lead"></span></p>
      <button class="delete" id="edit-team-lead-x" aria-label="close"></button>
    </header>
    <section class="modal-card-body">
      <table class="table is-fullwidth has-text-centered mb-6">
        <tr>
          <th>Select {{casemanager.username}}'s Team Lead</th>

          <th>
            <div class="dropdown is-down" id="team-lead-dropdown">
              <div class="dropdown-trigger">
                <button class="button" aria-haspopup="true" aria-controls="dropdown-menu">
                  <span id="selected-team-lead">Select Team Lead</span>
                  <span class="icon is-small">
                    <i class="fas fa-angle-down" aria-hidden="true"></i>
                  </span>
                </button>
              </div>

              <div class="dropdown-menu" id="team-lead-dropdown-menu" role="menu">
                <div class="dropdown-content" id="team-lead-role">


                  {{#each teamLeads}}
                  <div class="dropdown-item" value={{user_id}} id={{user_id}}>
                    {{username}}
                  </div> {{/each}}
                </div>
              </div>
            </div>
          </th>
      </table>
    </section>
    <footer class="modal-card-foot">
      <button class="button is-success" id="edit-team-lead-save">Save</button>
      <button class="button" id="edit-team-lead-cancel">Cancel</button>
    </footer>
  </div>
</div>




