{{!-- 
Begin Layout --}}
<div class="tile is-ancestor ml-3 mr-3">
  <div class="tile is-vertical">
    <div class="tile">
      <div class="tile is-parent is-vertical is-3" >
        <article class="tile is-child box p-0 message is-link">
          <div class="message-header">
    <p>My Caseload</p></div>
    <div class="container has-text-centered">
    {{#each cmCaseload}}
        {{#each UACs}}
        {{#if discharged }}
        {{else}}
        <ul>
          <li class="mt-2 mb-2 title is-4">
            <a href = "/dashboard/{{uac_id}}" style="text-decoration: none;">
        {{a_number}}</a>
        </li>
        </ul>
        {{/if}}
        {{/each}}
        {{/each}}
        </li>
        </article>

      </div>
      <div class="tile is-parent is-vertical">
        <article class="tile is-child box p-0 message is-link">
<div class="message-header">
    <p>UAC</p><div class="icon is-right"><i class="fa-solid fa-pen-to-square" id="edit-uac"></i></div></div>
    <table class="table is-fullwidth has-text-centered" >
      <thead>
        <tr>
          <th class="has-text-right">COO</th>
          <td class="has-text-left">{{uac.coo}}</td>
          <th class="has-text-right">DOB</th>
          <td class="has-text-left">{{uac.dob}}</td>
        </tr>
        <tr>
          <th class="has-text-right">LOS</th>
          <td class="has-text-left">{{uac.intake}}</td>
          <th class="has-text-right">Age</th>
          <td class="has-text-left">{{uac.age}}</td>
          
        </tr>
        <tr>
          <th class="has-text-right">Category</th>
          <td class="has-text-left">{{uac.category}}</td>
          <th class="has-text-right">Gender</th>
          <td class="has-text-left">{{uac.gender}}</td>
        </tr>
        <tr>
          <th class="has-text-right">SIR</th>
          <td class="has-text-left">{{uac.sir}}</td>
          <th class="has-text-right"></th>
          <td class="has-text-left"></td>

        </tr>
      </thead>
    </table>
        </article>        
        
<article class="tile is-child box p-0 message is-link">
<div class="message-header">
    <p>Sponsor</p><div class="icon is-right"><i class="fa-solid fa-pen-to-square" id="edit-sponsor"></i></div></div>
    <table class="table is-fullwidth has-text-centered" >
      <thead>
        <tr>
          <th class="has-text-right">Name</th>
          <td class="has-text-left">{{uac.sponsor_name}}</td>
          <th class="has-text-right">Address</th>
          <td class="has-text-left">{{uac.sponsor_address_1}}</td>
        </tr>
        <tr>
          <th class="has-text-right">City</th>
          <td class="has-text-left">{{uac.sponsor_city}}</td>
          <th class="has-text-right">State</th>
          <td class="has-text-left">{{uac.sponsor_state}}</td>
         
        </tr>
        <tr>
          <th class="has-text-right">Zip</th>
          <td class="has-text-left">{{uac.sponsor_zip}}</td>
          <th class="has-text-right">Gender</th>
          <td class="has-text-left">{{uac.sponsor_gender}}</td>
        </tr>
        <tr>
          <th class="has-text-right">Age</th>
          <td class="has-text-left">{{uac.sponsor_age}}</td>
          <th class="has-text-right">Relationship</th>
          <td class="has-text-left">{{uac.sponsor_relationship}}</td>
        </tr>
      </thead>
    </table>
        </article>  
     <article class="tile is-child box p-0 message is-link">
<div class="message-header">
    <p>Exception</p></div>
    <p>{{uac.exception}}</p>
        </article>
        <article class="tile is-child box p-0 message is-link">
<div class="message-header" id = "edit-status">
    <p>Case Status</p><div class="icon is-right"><i class="fa-solid fa-pen-to-square" id="edit-sponsor"></i></div></div>
    
        </article>
      </div>
    </div>

  </div>
  {{!-- Start Taskbar --}}
  <div class="tile is-parent is-3">
 <article class="tile is-child box p-0 message is-link">
          <div class="message-header">
    <p>Taskbar</p> <div class="icon is-right"><i class="fa-solid fa-pen-to-square" id="edit-tasks"></i></div></div>
     <table class="table is-fullwidth has-text-centered" >
      <thead>
        <tr>
          <th class="has-text-right">Sponsor Assessment</th>
          <td class="has-text-left" id = "sa-container">{{format_nice_date uac.sponsor_assessment}}</td>
        </tr>
        <tr>
          <th class="has-text-right">FRP</th>
          <td class="has-text-left" id = "frp-container"><span id = "display-frp">{{format_nice_date uac.frp}}</span> </td>
        </tr>
        <tr>
          <th class="has-text-right">ARI</th>
          <td class="has-text-left" id = "ari-container"><span id ="display-ari">{{format_nice_date uac.ari}}</span></td>
        </tr>
        <tr>
          <th class="has-text-right">Proof of Relationship</th>
          <td class="has-text-left" id = "por-container"><span id = "display-por">{{format_nice_date uac.por}} </span></td>
        </tr>
        <tr>
          <th class="has-text-right">Proof of Address</th>
          <td class="has-text-left" id = "poa-container">{{format_nice_date uac.poa}}</td>
        </tr>
        <tr>
          <th class="has-text-right">Letter of Designation</th>
          <td class="has-text-left" id = "lod-container">{{format_nice_date uac.lod}}</td>
        </tr>
        <tr>
          <th class="has-text-right">LOPC</th>
          <td class="has-text-left" id = "lopc-container">{{format_nice_date uac.lopc}}</td>
        </tr>
        <tr>          
          <tr>
          <th class="has-text-right">Sponsor Background Check</th>
          <td class="has-text-left" id = "sponsor_bgc-container">{{format_nice_date uac.sponsor_bgc}}</td>
        </tr>
        <tr>
          <th class="has-text-right">Sponsor ID Received</th>
          <td class="has-text-left" id ="sponsor_id-container"><span id = "sponsor-id">{{format_nice_date uac.sponsor_id}}</span></td>
        </tr>
        <tr>
          <th class="has-text-right">Fingerprints Taken</th>
          <td class="has-text-left" id ="sponsor_fp-container"><span id = "sponsor-fp">{{format_nice_date uac.sponsor_fp}}</span></td>
        </tr>
        <tr>
          <th class="has-text-right">HHM ID's Received</th>
          <td class="has-text-left" id ="hhm_id-container"><span id = "hhm-id">{{format_nice_date uac.hhm_id}}</span></td>
        </tr>
        <tr>
          <th class="has-text-right">Household Member Background Checks</th>
          <td class="has-text-left" id ="hhm_checks-container"><span id = "hhm-checks">{{format_nice_date uac.hhm_checks}}</span></td>
        </tr>
        <tr>
          <th class="has-text-right">Sex-Offender Checks</th>
          <td class="has-text-left" id ="sex_offender_check-container"><span id = "sex-offender-check">{{format_nice_date uac.sex_offender_check}}</span></td>
        </tr>
        <tr>
          <th class="has-text-right">Contacted COO Caregiver</th>
          <td class="has-text-left" id = "coo_caregiver_date-container">{{format_nice_date uac.coo_caregiver_date}}</td>
        </tr>
        <tr>
          <th class="has-text-right">CA/N Checks Requested</th>
          <td class="has-text-left" id = "can_check_requested_date-container">{{format_nice_date uac.can_check_requested_date}}</td>
        </tr>
        <tr>
          <th class="has-text-right">Prior Sponsorship</th>
          <td class="has-text-left" id ="prior_sponsorship_date-container">{{format_nice_date uac.prior_sponsorship_date}}</td>
        </tr>
        <tr>
          <th class="has-text-right">Previous Address</th>
          <td class="has-text-left" id ="previous_address_date-container">{{format_nice_date uac.previous_address_date}}</td>
        </tr>
        <tr>
          <th class="has-text-right">Criminal History</th>
          <td class="has-text-left" id ="criminal_history_date-container">{{format_nice_date uac.criminal_history_date}}</td>
        </tr>
      </thead>
    </table>   
    </article>
  </div>
  {{!-- End Taskbar --}}
</div>
{{!-- End Layout --}}
  

