    <div class="modal" id="edit-uac-modal">
      <div class="modal-background" id="edit-uac-BG"></div>
      <div class="modal-card">
        <header class="modal-card-head">
          <p class="modal-card-title">Edit UAC Info</p>
          <button
            class="delete"
            id="edit-uac-close-x"
            aria-label="close"
          ></button>
        </header>
        <section class="modal-card-body">

          <!-- Content ... -->
          Name:
          <input
          
            class="input"
            name="uacname"
            type="text"
            id= "uac-edit-name"
            placeholder={{uac.uacname}}>
          A#
          <input class="input" id = "a-number" name="a_number" type="text" placeholder= {{uac.a_number}}>
          DOB:
          <input class="input" id = "uac-dob" name="dob" type="text" placeholder={{uac.dob}} >
          COO:
          <input class="input" id = "uac-coo" name="coo" type="text" placeholder={{uac.coo}} >
          Age:
          <input class="input" id = "uac-age" name="age" type="text" placeholder={{uac.age}} >
          Category:
          <input
            class="input"
            id = "category"
            name="category"
            type="text"
            placeholder={{uac.category}}
          >
          Gender:
          <input
            class="input"
            id = "gender"
            name="gender"
            type="text"
            placeholder={{uac.gender}}
          >
        </section>
        <footer class="modal-card-foot">
          <button class="button is-success" id="edit-uac-btn">
            Save Changes
          </button>
          <button class="button" id="cancel-edit-uac-modal">Cancel</button>
        </footer>
      </div>
    </div>

